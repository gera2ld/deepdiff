{
  "name": "@gera2ld/deepdiff",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "scripts": {
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del dist types",
    "build:js": "rollup -c rollup.conf.js",
    "prebuild": "npm run ci && npm run clean",
    "prepublishOnly": "npm run build",
    "ci": "npm run lint && npm run test",
    "lint": "eslint --ext .ts .",
    "build": "tsc && npm run build:js",
    "test": "cross-env BABEL_ENV=test tape -r ./test/mock/register 'test/**/*.test.js'",
    "cov": "nyc --reporter=text --reporter=html npm test",
    "cov:open": "open coverage/index.html"
  },
  "publishConfig": {
    "access": "public"
  },
  "typings": "types/index.d.ts",
  "nyc": {
    "include": [
      "src/**"
    ]
  },
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@babel/register": "^7.10.5",
    "@gera2ld/plaid": "~2.0.7",
    "@gera2ld/plaid-common-ts": "^2.0.7",
    "@gera2ld/plaid-rollup": "~2.0.7",
    "babel-plugin-istanbul": "^6.0.0",
    "del-cli": "^3.0.1",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "husky": "^4.2.5",
    "nyc": "^15.1.0",
    "tape": "^5.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  }
}
